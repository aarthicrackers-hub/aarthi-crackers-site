{
  "rewrites": [
    {
      "source": "/((?!logo\\.png|favicon\\.ico|robots\\.txt|sitemap\\.xml|AARTHI-CRACKERS-Pricelist\\.pdf).*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/logo.png",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        },
        {
          "key": "Content-Type",
          "value": "image/png"
        }
      ]
    },
    {
      "source": "/AARTHI-CRACKERS-Pricelist.pdf",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        },
        {
          "key": "Content-Type",
          "value": "application/pdf"
        },
        {
          "key": "Content-Disposition",
          "value": "attachment; filename=\"AARTHI-CRACKERS-Pricelist.pdf\""
        }
      ]
    }
  ]
}
